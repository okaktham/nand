/**
 * 8-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[7]
 */
CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0], b=in[1], out=w0);
    Or(a=in[2], b=in[3], out=w1);
    Or(a=in[3], b=in[4], out=w2);
    Or(a=in[4], b=in[5], out=w3);
    Or(a=w0, b=w1, out=o1);
    Or(a=w2, b=w3, out=o2);
    Or(a=o1, b=o2, out=out);

}